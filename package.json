{
	"name": "@golemio/integration-engine",
	"version": "1.11.4",
	"description": "Integration Engine of the Golemio Data Platform System",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"dev-start": "ts-node-dev -r dotenv/config ./src/index.ts",
		"dev-start-debug": "ts-node-dev --inspect-brk=9230 -r dotenv/config ./src/index.ts",
		"start": "node -r dotenv/config dist/index.js",
		"debug": "node --inspect-brk=9230 -r dotenv/config dist/index.js",
		"build": "tsc",
		"build-minimal": "tsc --sourceMap false --declaration false",
		"build-watch": "tsc --watch --preserveWatchOutput",
		"test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/core/**/*.test.ts test/modules/**/*.test.ts test/App.test.ts --exclude test/datasources-test/*.{ts,js,txt} --exit",
		"test-debug": "mocha --inspect-brk=9230 --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/core/**/*.test.ts test/modules/**/*.test.ts test/App.test.ts --exclude test/datasources-test/*.{ts,js,txt} --exit",
		"core-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/core/**/*.test.ts test/App.test.ts --exit",
		"modules-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/modules/**/*.test.ts --exit",
		"code-coverage": "nyc mocha --timeout 120000 -r ts-node/register -r source-map-support/register -r dotenv/config test/core/**/*.test.ts test/modules/**/*.test.ts test/App.test.ts --exclude test/datasources-test/*.{ts,js,txt} --exit",
		"datasources-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/datasources-test/DataSourcesAvailabilityChecking.test.ts --exit",
		"datasources-test-report": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/datasources-test/DataSourcesAvailabilityChecking.test.ts --exit > test/datasources-test/report.txt ; node -r dotenv/config test/datasources-test/reportErrorsByEmail.js",
		"generate-docs": "npx typedoc --readme \"./README.MD\" --name \"Golemio Integration Engine\" --mode file --out docs/typedoc src",
		"tslint": "tslint -p ."
	},
	"keywords": [],
	"author": "Operator ICT, a.s.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/operator-ict/golemio/code/integration-engine"
	},
	"devDependencies": {
		"@types/amqplib": "^0.5.13",
		"@types/chai": "4.2.3",
		"@types/chai-as-promised": "7.1.2",
		"@types/debug": "0.0.31",
		"@types/decompress": "^4.2.3",
		"@types/express": "^4.17.0",
		"@types/ioredis": "4.0.10",
		"@types/jsonwebtoken": "^8.3.8",
		"@types/lodash": "^4.14.165",
		"@types/mocha": "5.2.5",
		"@types/moment": "^2.13.0",
		"@types/moment-timezone": "^0.5.12",
		"@types/mongoose": "5.3.1",
		"@types/morgan": "^1.7.35",
		"@types/node": "^14.0.0",
		"@types/nodemailer": "^6.2.1",
		"@types/object-hash": "^1.3.0",
		"@types/request-promise": "^4.1.45",
		"@types/sequelize": "^4.28.4",
		"@types/sinon": "^9.0.10",
		"@types/supertest": "2.0.4",
		"@types/underscore": "^1.9.3",
		"chai": "4.2.0",
		"chai-as-promised": "7.1.1",
		"husky": "^3.0.0",
		"mocha": "5.2.0",
		"nock": "10.0.6",
		"nyc": "13.1.0",
		"sinon": "^9.2.3",
		"source-map-support": "0.5.9",
		"ts-node": "^8.3.0",
		"ts-node-dev": "^1.0.0-pre.40",
		"tslint": "5.11.0",
		"typedoc": "^0.17.8",
		"typescript": "3.8.2"
	},
	"dependencies": {
		"@golemio/errors": "^0.0.5",
		"@golemio/schema-definitions": "1.9.1-dev.260966080",
		"@golemio/utils": "^0.0.3",
		"@golemio/validator": "0.2.0",
		"@google-cloud/storage": "4.5.0",
		"@turf/turf": "5.1.6",
		"@types/proj4": "^2.5.0",
		"@types/ssh2-sftp-client": "^5.3.0",
		"JSONStream": "^1.3.5",
		"amqplib": "0.5.2",
		"aws-sdk": "^2.657.0",
		"axios": "",
		"basic-ftp": "2.17.0",
		"cheap-ruler": "2.5.1",
		"content-type": "^1.0.4",
		"csv-parser": "2.3.2",
		"debug": "3.1.0",
		"decompress": "4.2.0",
		"dotenv": "6.1.0",
		"express": "^4.17.1",
		"fast-csv": "3.0.1",
		"gtfs-realtime-bindings": "^0.0.5",
		"iconv-lite": "0.4.24",
		"influx": "5.4.0",
		"ioredis": "4.6.2",
		"iso8601-duration": "^1.2.0",
		"jsonwebtoken": "8.5.1",
		"lodash": "^4.17.19",
		"moment-timezone": "^0.5.26",
		"mongoose": "5.3.6",
		"morgan": "^1.9.1",
		"nodemailer": "^6.3.0",
		"object-hash": "^2.0.0",
		"pg": "7.6.1",
		"pg-hstore": "2.3.2",
		"proj4": "^2.6.2",
		"request": "2.88.0",
		"request-promise": "^4.2.5",
		"sequelize": "4.41.2",
		"slugify": "1.3.2",
		"ssh2-sftp-client": "^5.3.1",
		"supertest": "4.0.2",
		"underscore": "1.9.1",
		"uuid": "^8.1.0",
		"uuid-by-string": "^3.0.2",
		"winston": "3.2.1",
		"xml-to-json-stream": "^1.1.0",
		"xml2js-es6-promise": "^1.1.1"
	},
	"nyc": {
		"check-coverage": true,
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"**/*.d.ts",
			"coverage",
			"dist",
			"docs",
			"node_modules",
			"test"
		],
		"reporter": [
			"text",
			"lcov"
		],
		"all": true,
		"lines": 0,
		"statements": 0,
		"functions": 0,
		"branches": 0
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run tslint"
		}
	}
}
