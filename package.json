{
    "name": "@golemio/integration-engine",
    "version": "1.11.4",
    "description": "Integration Engine of the Golemio Data Platform System",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev-start": "ts-node-dev -r dotenv/config ./src/index.ts",
        "dev-start-debug": "ts-node-dev --inspect-brk=9230 -r dotenv/config ./src/index.ts",
        "start": "node -r dotenv/config dist/index.js",
        "debug": "node --inspect-brk=9230 -r dotenv/config dist/index.js",
        "build": "tsc",
        "build-minimal": "tsc --sourceMap false --declaration false",
        "build-watch": "tsc --watch --preserveWatchOutput",
        "test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/App.test.ts --exit",
        "test-debug": "mocha --inspect-brk=9230 --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/App.test.ts --exit",
        "code-coverage": "nyc mocha --timeout 120000 -r ts-node/register -r source-map-support/register -r dotenv/config test/App.test.ts --exit",
        "datasources-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/datasources-test/DataSourcesAvailabilityChecking.test.ts --exit",
        "datasources-test-report": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/datasources-test/DataSourcesAvailabilityChecking.test.ts --exit > test/datasources-test/report.txt ; node -r dotenv/config test/datasources-test/reportErrorsByEmail.js",
        "generate-docs": "npx typedoc --readme \"./README.MD\" --name \"Golemio Integration Engine\" --mode file --out docs/typedoc src",
        "lint": "eslint src/**/*.ts"
    },
    "keywords": [],
    "author": "Operator ICT, a.s.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/operator-ict/golemio/code/integration-engine"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
