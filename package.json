{
	"name": "golemio-integration-engine",
	"version": "0.1.6",
	"description": "Integration Engine of the Golemio data platform.",
	"main": "dist/App.js",
	"types": "dist/App.d.ts",
	"scripts": {
		"start": "node -r dotenv/config dist/App.js",
		"build": "tsc",
		"build-watch": "tsc --watch --preserveWatchOutput",
		"test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/core/helpers/*.test.ts test/core/helpers/**/*.test.ts test/core/**/*.test.ts test/modules/**/*.test.ts --exclude test/DataSourcesAvailabilityChecking.test.ts --exit",
		"core-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/core/**/*.test.ts test/core/helpers/**/*.test.ts --exit",
		"modules-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/modules/**/*.test.ts --exit",
		"datasources-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/DataSourcesAvailabilityChecking.test.ts --exit",
		"code-coverage": "nyc mocha --timeout 120000 -r ts-node/register -r source-map-support/register -r dotenv/config test/core/helpers/*.test.ts test/core/helpers/**/*.test.ts test/core/**/*.test.ts test/modules/**/*.test.ts --exclude test/DataSourcesAvailabilityChecking.test.ts --exit",
		"generate-docs": "npx typedoc --readme \"./README.MD\" --name \"Golemio Integration Engine\" --mode file --out docs/typedoc src"
	},
	"keywords": [],
	"author": "Operator ICT, a.s.",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://gitlab.oict.cz/data-platform/integration-engine"
	},
	"devDependencies": {
		"@types/debug": "0.0.31",
		"@types/ioredis": "4.0.10",
		"@types/node": "10.12.0",
		"@types/chai": "4.1.7",
		"@types/mocha": "5.2.5",
		"@types/sequelize": "4.27.32",
		"ts-node": "7.0.1",
		"typescript": "3.1.3",
		"tslint": "5.11.0",
		"chai": "4.2.0",
		"mocha": "5.2.0",
		"chai-as-promised": "7.1.1",
		"sinon": "7.1.1",
		"nyc": "13.1.0",
		"source-map-support": "0.5.9",
		"nock": "10.0.6",
		"typedoc": "^0.14.2"
	},
	"dependencies": {
		"amqplib": "0.5.2",
		"basic-ftp": "2.17.0",
		"cheap-ruler": "2.5.1",
		"csvtojson": "2.0.8",
		"golemio-schema-definitions": "0.1.8-dev.1821",
		"debug": "3.1.0",
		"decompress": "4.2.0",
		"dotenv": "6.1.0",
		"iconv-lite": "0.4.24",
		"ioredis": "4.6.2",
		"moment-timezone": "0.5.23",
		"mongoose": "5.3.6",
		"pg": "7.6.1",
		"pg-hstore": "2.3.2",
		"request": "2.88.0",
		"request-promise": "4.2.2",
		"sequelize": "4.41.2",
		"slugify": "1.3.2",
		"@turf/turf": "5.1.6",
		"underscore": "1.9.1",
		"winston": "3.2.1",
		"xml2js-es6-promise": "1.1.1"
	},
	"nyc": {
		"check-coverage": true,
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"**/*.d.ts",
			"dist",
			"**/*.test.ts",
			"coverage",
			"node_modules"
		],
		"reporter": [
			"text",
			"lcov"
		],
		"all": true,
		"lines": 0,
		"statements": 0,
		"functions": 0,
		"branches": 0
	}
}
