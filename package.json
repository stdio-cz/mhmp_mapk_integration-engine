        "@golemio/city-districts": "1.1.0",
        "@golemio/cli": "1.4.1",
        "@golemio/core": "1.5.0",
        "@golemio/energetics": "1.2.2",
        "@golemio/fcd": "1.1.0",
        "@golemio/firebase-pid-litacka": "1.1.0",
        "@golemio/flow": "1.2.0",
        "@golemio/gardens": "1.2.0",
        "@golemio/medical-institutions": "1.1.0",
