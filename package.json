{
	"name": "data-platform-integration-engine",
	"version": "0.0.2",
	"description": "Integration Engine of the Data Platform System",
	"main": "dist/App.js",
	"types": "dist/App.d.ts",
	"scripts": {
		"start": "node -r dotenv/config dist/App.js",
		"postinstall": "tsc",
		"build": "tsc",
		"build-watch": "tsc --watch --preserveWatchOutput",
		"test": "mocha --timeout 60000 -r ts-node/register -r dotenv/config test/**/*.test.ts --exit",
		"code-coverage": "nyc mocha --timeout 60000 -r ts-node/register -r source-map-support/register -r dotenv/config --recursive test/**/*.test.ts --exit"
	},
	"keywords": [],
	"author": "Operator ICT, a.s.",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://gitlab.oict.cz/data-platform/integration-engine"
	},
	"devDependencies": {
		"@types/debug": "0.0.31",
		"@types/node": "10.12.0",
		"@types/chai": "4.1.7",
		"@types/mocha": "5.2.5",
		"ts-node": "7.0.1",
		"typescript": "3.1.3",
		"tslint": "5.11.0",
		"chai": "4.2.0",
		"mocha": "5.2.0",
		"chai-as-promised": "7.1.1",
		"sinon": "7.1.1",
		"nyc": "13.1.0",
		"source-map-support": "0.5.9"
	},
	"dependencies": {
		"amqplib": "0.5.2",
		"csvtojson": "2.0.8",
		"data-platform-schema-definitions": "file:../schema-definitions",
		"debug": "3.1.0",
		"dotenv": "6.1.0",
		"mongoose": "5.3.6",
		"request": "2.88.0",
		"request-promise": "4.2.2",
		"slugify": "1.3.2",
		"underscore": "1.9.1"
	},
	"nyc": {
		"check-coverage": true,
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"**/*.d.ts"
		],
		"reporter": [
			"text-summary"
		],
		"all": true,
		"lines": 0,
		"statements": 0,
		"functions": 0,
		"branches": 0
	}
}
