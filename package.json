{
	"name": "data-platform-integration-engine",
	"version": "0.0.1",
	"description": "Integration Engine of the Data Platform System",
	"main": "dist/App.js",
	"types": "dist/App.d.ts",
	"scripts": {
		"start": "export DEBUG='*,-mquery' ; node dist/App.js",
		"build": "tsc",
		"build-watch": "tsc --watch --preserveWatchOutput",
		"test": "mocha --timeout 60000 --require ts-node/register test/*/*.test.ts --exit",
		"datasourcesTest": "mocha --timeout 600000 --require ts-node/register test/datasources/*DataSource.test.ts --exit > test/REPORTING/test.txt ; node test/REPORTING/reportErrors.js"
	},
	"keywords": [],
	"author": "Operator ICT, a.s.",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://gitlab.oict.cz/data-platform/integration-engine"
	},
	"devDependencies": {
		"@types/debug": "0.0.31",
		"@types/node": "10.12.0",
		"ts-node": "7.0.1",
		"typescript": "3.1.3",
		"tslint": "5.11.0"
	},
	"dependencies": {
		"amqplib": "0.5.2",
		"data-platform-schema-definitions": "file:../schema-definitions",
		"debug": "3.1.0",
		"mongoose": "5.3.6",
		"request": "2.88.0",
		"request-promise": "4.2.2",
		"underscore": "1.9.1"
	}
