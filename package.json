{
    "name": "@golemio/integration-engine",
    "version": "2.7.5",
    "description": "Integration Engine of the Golemio Data Platform System",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev-start": "ts-node-dev -r dotenv/config ./src/index.ts",
        "dev-start-debug": "ts-node-dev --inspect-brk=9230 -r dotenv/config ./src/index.ts",
        "start": "node -r dotenv/config dist/index.js",
        "debug": "node --inspect-brk=9230 -r dotenv/config dist/index.js",
        "build": "tsc",
        "build-minimal": "tsc --sourceMap false --declaration false",
        "build-watch": "tsc --watch --preserveWatchOutput",
        "test": "mocha --check-leaks --global classValidatorMetadataStorage --timeout 120000 -r ts-node/register --file 'test/setup.ts' -r dotenv/config test/*.test.ts --exit",
        "test-debug": "mocha --inspect-brk=9230 --check-leaks --global classValidatorMetadataStorage --timeout 120000 -r ts-node/register --file 'test/setup.ts' -r dotenv/config test/*.test.ts --exit",
        "code-coverage": "nyc mocha --timeout 120000 -r ts-node/register -r source-map-support/register -r dotenv/config test/*.test.ts --file 'test/setup.ts' --exit --reporter mocha-junit-reporter --reporter-options mochaFile=junit.xml",
        "datasources-test": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config test/datasources-test/DataSourcesAvailabilityChecking.test.ts --exit",
        "datasources-test-report": "mocha --check-leaks --timeout 120000 -r ts-node/register -r dotenv/config --file test/datasources-test/reportErrors.ts test/datasources-test/DataSourcesAvailabilityChecking.test.ts --exit",
        "generate-docs": "typedoc --out docs/typedoc src",
        "lint": "eslint src/**/*.ts"
    },
    "keywords": [],
    "author": "Operator ICT, a.s.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/operator-ict/golemio/code/integration-engine"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0",
        "yarn": "Use npm!"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@golemio/eslint-config": "1.1.0",
        "@google-cloud/storage": "^5.7.1",
        "@types/chai": "4.2.3",
        "@types/chai-as-promised": "7.1.2",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/mocha": "^8.2.1",
        "@types/node": "^18.13.0",
        "@types/nodemailer": "^6.4.1",
